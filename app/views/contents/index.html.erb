<h1>Contents#index</h1>
<p>Find me in app/views/contents/index.html.erb</p>
<%= link_to '새 글 쓰기', new_content_path %>

<%= @contents.each do |content| %>
    <a href='contents/<% content.id %>'><%= content.title %></a>
    
    <%= link_to '글 수정하기', edit_content_path(content) %>
    <%= link_to '삭제하기', content_path(content), method: :delete, 
        data: {confirm: '정말 삭제하시겠습니까?'}%>
    <br>
    
<% end %>
